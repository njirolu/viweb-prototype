{
  "name": "be-wasm",
  "version": "1.0.0",
  "description": "Express + SQLite (WASM) API and frontend demo for WebContainer",
  "main": "server/app.js",
  "scripts": {
    "dev": "node server/app.js",
    "dev:container:api": "WEB_CONTAINER_TARGET=1 node server/app.js",
    "dev:container:notes": "vite --config vite.notes.config.ts --host 0.0.0.0 --port 4173 --strictPort",
    "dev:container": "concurrently -k -n api,notes \"npm run dev:container:api\" \"npm run dev:container:notes\"",
    "start": "node server/app.js",
    "test": "node --test",
    "build:notes": "vite build --config vite.notes.config.ts",
    "build:runner-ui": "vite build --config vite.runner.config.ts",
    "build:runner": "npm run build:notes && node scripts/generate-webcontainer-files.js && npm run build:runner-ui",
    "typecheck:runner": "tsc --project tsconfig.runner.json --noEmit",
    "typecheck:notes": "tsc --project tsconfig.notes.json --noEmit",
    "typecheck": "npm run typecheck:runner && npm run typecheck:notes"
  },
  "keywords": [
    "express",
    "sqlite",
    "sqljs",
    "webcontainer",
    "wasm"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "lucide-react": "^0.548.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sql.js": "^1.14.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.0.2",
    "@webcontainer/api": "^1.6.1",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "monaco-editor": "^0.55.1",
    "postcss": "^8.5.6",
    "supertest": "^7.2.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "vite": "^7.3.1",
    "xterm": "^5.3.0"
  }
}
